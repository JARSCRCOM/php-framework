{
    "name": "jarscr/php-framework",
    "type": "project",
    "description": "PHP MVC framework for building web applications in PHP 7.3",
    "keywords": ["mvc","framework","php","costa rica","web"],
    "homepage": "https://github.com/jarscr/php-framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Alfredo Rodr√≠guez",
            "email": "jarscr@gmail.com",
            "homepage": "http://www.jarscr.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.3",
        "twig/twig": "~3.0",
        "symfony/polyfill-mbstring": "^1.10",       
        "psr/log": "^1.1",
        "mpdf/mpdf": "^8.0",
        "delight-im/auth": "^8.2",
        "phpoffice/phpspreadsheet": "^1.14",
        "doctrine/annotations": "^1.10",
        "doctrine/collections": "^1.6",
        "doctrine/migrations": "^3.0",
        "doctrine/common": "^3.0",
        "doctrine/inflector": "^2.0",
        "symfony/yaml": "^5.1",
        "sentry/sdk": "^3.0",
        "aws/aws-sdk-php": "^3.171"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
             "Core\\": "Core/",
             "App\\": "App/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}